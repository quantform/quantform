{
  "name": "@quantform/startup-project",
  "packageManager": "yarn@3.0.2",
  "description": "Node.js library for building systematic trading strategies in reactive way.",
  "author": "Mateusz Majchrzak",
  "license": "MIT",
  "homepage": "https://quantform.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/quantform/quantform"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "yarn run qf dev dca",
    "build": "tsc",
    "test:watch": "jest --watch",
    "test:unit": "jest -c jestconfig.unit.json --coverage --passWithNoTests",
    "test:integration": "jest -c jestconfig.integration.json --setupFiles dotenv/config"
  },
  "dependencies": {
    "@quantform/binance": "workspace:packages/adapter/binance",
    "@quantform/core": "workspace:packages/core",
    "@quantform/sqlite": "workspace:packages/storage/sqlite",
    "@quantform/studio": "workspace:packages/studio",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.169",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">= 14.18.1"
  }
}
