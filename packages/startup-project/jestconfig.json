{
  "moduleFileExtensions": ["js", "json", "ts"],
  "verbose": true,
  "rootDir": "src",
  "testRegex": ".spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "coverageDirectory": "../coverage",
  "testEnvironment": "node",
  "testPathIgnorePatterns": ["tests"],
  "testTimeout": 100000
}
