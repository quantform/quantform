{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/storage/sqlite/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/storage/sqlite",
        "main": "packages/storage/sqlite/src/index.ts",
        "tsConfig": "packages/storage/sqlite/tsconfig.lib.json",
        "assets": ["packages/storage/sqlite/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/storage/sqlite/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/storage/sqlite"],
      "options": {
        "jestConfig": "packages/storage/sqlite/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
