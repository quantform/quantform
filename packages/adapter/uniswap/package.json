{
  "name": "@quantform/uniswap",
  "version": "0.2.2",
  "description": "Node.js library for building systematic trading strategies in reactive way.",
  "author": "Mateusz Majchrzak",
  "license": "MIT",
  "homepage": "https://quantform.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/quantform/quantform"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "test:watch": "jest --watch",
    "test:unit": "jest -c jestconfig.unit.json --passWithNoTests",
    "test:integration": "jest -c jestconfig.integration.json",
    "test:cov": "jest -c jestconfig.json --coverage --setupFiles dotenv/config"
  },
  "dependencies": {
    "@ethersproject/address": "^5.3.0",
    "@ethersproject/contracts": "^5.3.0",
    "@ethersproject/networks": "^5.3.1",
    "@ethersproject/providers": "^5.3.1",
    "@ethersproject/solidity": "^5.3.0",
    "@quantform/core": "workspace:packages/core",
    "@uniswap/sdk": "^3.0.3"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "eslint": "^8.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">= 14.18.1"
  }
