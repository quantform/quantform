{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/adapter/dydx/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/adapter/dydx",
        "main": "packages/adapter/dydx/src/index.ts",
        "tsConfig": "packages/adapter/dydx/tsconfig.lib.json",
        "assets": ["packages/adapter/dydx/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/adapter/dydx/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/adapter/dydx"],
      "options": {
        "jestConfig": "packages/adapter/dydx/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
