{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/adapter/binance/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/adapter/binance",
        "main": "packages/adapter/binance/src/index.ts",
        "tsConfig": "packages/adapter/binance/tsconfig.lib.json",
        "assets": ["packages/adapter/binance/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/adapter/binance/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/adapter/binance"],
      "options": {
        "jestConfig": "packages/adapter/binance/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
