{
  "name": "@quantform/express",
  "version": "0.1.0",
  "description": "Node.js library for building systematic trading strategies in reactive way.",
  "author": "Mateusz Majchrzak",
  "license": "MIT",
  "homepage": "https://quantform.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/quantform/quantform"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "start": "tsc && node dist/index.js",
    "test:watch": "jest --watch",
    "test:unit": "jest -c jestconfig.unit.json",
    "test:integration": "jest -c jestconfig.integration.json --setupFiles dotenv/config",
    "test:cov": "jest -c jestconfig.json --coverage --setupFiles dotenv/config"
  },
  "dependencies": {
    "@types/cors": "^2.8.10",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "class-validator-jsonschema": "^3.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "multer": "^1.4.2",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "routing-controllers-openapi": "^3.0.0",
    "swagger-ui-express": "^4.1.6",
    "tslib": "^1.14.1",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^27.0.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^16.10.3",
    "@types/swagger-ui-express": "^4.1.2",
    "eslint": "^8.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
