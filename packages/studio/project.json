{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/studio/src",
  "projectType": "library",
  "implicitDependencies": ["studio-app"],
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/studio",
        "main": "packages/studio/src/index.ts",
        "tsConfig": "packages/studio/tsconfig.lib.json",
        "assets": ["packages/studio/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/studio/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/studio"],
      "options": {
        "jestConfig": "packages/studio/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
